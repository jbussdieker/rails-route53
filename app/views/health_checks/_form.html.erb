<%= form_for(@health_check, html: {class: 'form-horizontal'}) do |f| %>
  <div class="form-group<%= " has-error" if @health_check.errors.messages.has_key? :health_check_id %>">
    <%= f.label :health_check_id, class: 'control-label' %>
    <%= f.text_field :health_check_id, class: 'form-control' %>
    <% @health_check.errors.messages[:health_check_id].each do |message| %>
      <%= f.label message, class: 'help-block' %>
    <% end %>
  </div>
  <div class="form-group<%= " has-error" if @health_check.errors.messages.has_key? :ip_address %>">
    <%= f.label :ip_address, class: 'control-label' %>
    <%= f.text_field :ip_address, class: 'form-control' %>
    <% @health_check.errors.messages[:ip_address].each do |message| %>
      <%= f.label message, class: 'help-block' %>
    <% end %>
  </div>
  <div class="form-group<%= " has-error" if @health_check.errors.messages.has_key? :port %>">
    <%= f.label :port, class: 'control-label' %>
    <%= f.number_field :port, class: 'form-control' %>
    <% @health_check.errors.messages[:port].each do |message| %>
      <%= f.label message, class: 'help-block' %>
    <% end %>
  </div>
  <div class="form-group<%= " has-error" if @health_check.errors.messages.has_key? :type %>">
    <%= f.label :type, class: 'control-label' %>
    <%= f.text_field :type, class: 'form-control' %>
    <% @health_check.errors.messages[:type].each do |message| %>
      <%= f.label message, class: 'help-block' %>
    <% end %>
  </div>
  <div class="form-group<%= " has-error" if @health_check.errors.messages.has_key? :resource_path %>">
    <%= f.label :resource_path, class: 'control-label' %>
    <%= f.text_field :resource_path, class: 'form-control' %>
    <% @health_check.errors.messages[:resource_path].each do |message| %>
      <%= f.label message, class: 'help-block' %>
    <% end %>
  </div>
  <div class="form-group<%= " has-error" if @health_check.errors.messages.has_key? :fully_qualified_domain_name %>">
    <%= f.label :fully_qualified_domain_name, class: 'control-label' %>
    <%= f.text_field :fully_qualified_domain_name, class: 'form-control' %>
    <% @health_check.errors.messages[:fully_qualified_domain_name].each do |message| %>
      <%= f.label message, class: 'help-block' %>
    <% end %>
  </div>
  <div class="form-group">
    <%= f.submit nil, class: 'btn btn-primary' %>
  </div>
<% end %>
